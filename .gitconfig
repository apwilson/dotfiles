[github]
  user = jxson
[core]
  excludesfile = ~/.gitignore
  quotepath = false
  editor = atom --wait
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[http]
  sslVerify = true
  cookiefile = ~/.gitcookies
[push]
  default = simple
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
[alias]
  zap-branches = !sh ~/.dotfiles/git-zap-branches.sh
[transfer]
  fsckobjects = true
[fetch]
  fsckobjects = true
[receive]
  fsckObjects = true
[interactive]
	diffFilter = diff-highlight
[color "diff-highlight"]
	oldNormal = red bold
	oldHighlight = red bold 52
	newNormal = green bold
	newHighlight = green bold 22
[diff]
  compactionHeuristic = true
