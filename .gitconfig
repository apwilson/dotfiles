[user]
  name = Jason Campbell
  email = jasoncampbell@google.com
  username = jxson
[github]
  user = jxson
[core]
  excludesfile = ~/.gitignore
  quotepath = false
  editor = atom --wait
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[http]
  sslVerify = true
  cookiefile = ~/.gitcookies
[push]
  default = simple
[ghi]
  token = !security find-internet-password -a jxson -s github.com -l 'ghi token' -w
[filter "lfs"]
  clean = git-lfs clean %f
  smudge = git-lfs smudge %f
  required = true
